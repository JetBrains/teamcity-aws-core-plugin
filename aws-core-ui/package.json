    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^15.0.0",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
