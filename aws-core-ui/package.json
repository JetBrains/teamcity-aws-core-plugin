    "@jetbrains/icons": "^4.2.0",
    "@jetbrains/ring-ui": "^5.0.108",
    "@jetbrains/teamcity-api": "^2022.4.1-v.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "json5": "^2.2.3",
    "react-hook-form": "^7.43.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
