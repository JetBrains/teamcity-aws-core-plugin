{
  "name": "@jetbrains-internal/aws-connection-components",
  "version": "0.1.49",
  "description": "React UI for AWS Connection configuration",
  "main": "dist/components.js",
  "types": "dist/components.d.ts",
  "author": "andrei.efimov@jetbrains.com",
  "license": "APACHE 2.0",
  "scripts": {
    "build:ts": "rm -rf ./dist && tsc",
    "build:types": "node dts-bundle-script.js",
    "prepub": "yarn build:ts && yarn build:types && yarn build",
    "pub": "yarn publish --registry https://packages.jetbrains.team/npm/p/tc/teamcity-npm/ --auth-type legacy",
    "start": "webpack serve --env devserver --port=8081 --config-name app",
    "build": "webpack --env production --env analyze",
    "docker-start": "npm install && npm run start -- --port=8080 --host 0.0.0.0",
    "docker-build": "npm install && npm run build",
    "lint:js": "eslint",
    "lint": "yarn lint:js . && yarn stylelint"
  },
  "files": [
    "dist",
    "src",
    "package.json"
  ],
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@hookform/devtools": "^4.3.0",
